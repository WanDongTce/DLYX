<!--pages/home/finance/allIncreaseAgentDetail/allIncreaseAgentDetail.wxml-->
<view class='bigbox'>
  
  <view class='item_box'>
    
    <view class='item display-flex'>
      <view class='item_left'>代理姓名</view>
      <view class='item_right'>{{name}}</view>
    </view>
    <view class='item display-flex'>
      <view class='item_left'>代理类型</view>
      <view class='item_right'>{{agenttype}}</view>
    </view>
    <view class='item display-flex'>
      <view class='item_left'>代理区域</view>
      <view class='item_right'>{{area}}</view>
    </view>
    <view class='item display-flex'>
      <view class='item_left'>推<text class='item_ml_3'>荐</text>人</view>
      <view class='item_right'>{{parentagent}}</view>
    </view>
    
    
  </view>
</view>