  font-size:24rpx;
  color:#939393;
  line-height:60rpx;
  width:100%;
  border-radius:5rpx;
}
.sidebar_input_box input{
  font-size:24rpx;
  color:#939393;
  line-height:60rpx;
  margin-left:27rpx;
  margin-right:27rpx;
  width:100%;
}
.sidebar_input_placeholder{
  color:#8d8d8d;
}
.sidebar_bottom_box{
  position:absolute;
  bottom:30rpx;
  width:100%;
}
.sidebar_bottom{
  border:1rpx solid #4d4d4d; 
  box-sizing: border-box;
  line-height:76rpx;
  width:442rpx;
  margin:0 auto;
  border-radius: 5rpx;
}
.sidebar_bottom_left{
  width:50%;
  font-size:29rpx;
  text-align: center;
  color:#4d4d4d;
}
.sidebar_bottom_right{
  width:50%;
  font-size:29rpx;
  text-align: center;
  color:#fff;
  background-color:#4d4d4d;
}
.incometypebg1{
  background-color:red;
}
.incometypebg2{
  background-color:#f1f1f1;
}

.r_red_text{
  color:#ED1B1A !important;
}

