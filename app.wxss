/* 外层容器 */

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
    font-family: square, Arial, Helvetica, sans-serif, Times, serif, "微软雅黑", "华文细黑";
}

/* dialog  */

.detail-dialog-mask {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 30;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.4);
    overflow: hidden;
}

.detail-container {
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 40;
    width: 100%;
    min-height: 500rpx;
    overflow: hidden;
    padding: 00rpx 0;
    background-color: #fff;
}

.detail-content {
    width: 100%;
    /* padding: 0 40rpx 40rpx 40rpx; */
    background-color: #fff;
    /* border-bottom: 1rpx solid #f2f2f2; */
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}

.detail-title {
    text-align: center;
    color: #2e2e2e;
    font-weight: bold;
    font-size: 30rpx;
    margin: 40rpx 0;
}

.detail-item {
    line-height: 60rpx;
    color: #333;
    font-size: 28rpx;
    margin-left:210rpx;
    font-weight:bold;
}

.dialog-btns {
    width: 100%;
    padding-top: 20rpx;
}

.dialog-btns > navigator {
    width: 60%;
    height: 80rpx;
    line-height: 80rpx;
    color: #fff;
    font-size: 28rpx;
    text-align: center;
    margin: 20rpx auto;
    border-radius: 20rpx;
}
.rdialog_top{text-align: center;font-size:30rpx;color:#333;line-height:86rpx;border-bottom:1rpx solid #dfdfdf;margin-bottom:30rpx;font-weight:bold;}
.rdialog_text{
  font-size:26rpx;color:#333;line-height:60rpx;font-weight: normal;
  width: 350rpx;white-space:nowrap; overflow:hidden;text-overflow:ellipsis;
}
/* dialog  end */

/* directAgency tab  */
.header-tab{
	position: fixed;
	top: 0;
	left: 0;
}

.tab-container > view {
    width: 50%;
