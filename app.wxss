/* search */
.search-container{
	position: fixed;
	top: 0;
	left: 0; 
	z-index: 30;
	width: 100%;
	height: 50px;
	line-height: 50px;
	text-align: center;
	font-size: 28rpx;
	color: #2e2e2e;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
	background-color: #5cb5ff;
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #74c0fb), color-stop(100%, #48a1f9));
    background: -webkit-linear-gradient(top, #74c0fb 0%, #48a1f9 100%);
    background: linear-gradient(to bottom, #74c0fb 0%, #48a1f9 100%);
}
/* search  end */

/* student or agent table and select */

.icon-arrow {
    width: 0;
    height: 0;
    border: 5px solid;
    border-top-color: #fff;
    border-left-color: transparent;
    border-right-color: transparent;
    border-bottom-color: transparent;
    display: inline-block;
    vertical-align: middle;
    margin-top: 12rpx;
}
.icon-arrow-up {
    width: 0;
    height: 0;
    border: 5px solid;
    border-bottom-color: #fff;
    border-left-color: transparent;
    border-right-color: transparent;
    border-top-color: transparent;
    display: inline-block;
    vertical-align: middle;
     margin-bottom: 15rpx; 
}

.select {
    position: fixed;
    top: 50px;
    left: 0;
    z-index: 20;
    width: 100%;
    background: rgba(0, 0, 0, 0.5);
}
.select-content {
    width: 25%;
	height: 100%;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    margin: 0;
    overflow-y: scroll;
}

.select-content > view {
    width: 100%;
    padding: 0 2%;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 28rpx;
    color: #2e2e2e;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
}

.select-dialog-btns {
    position: fixed;
    bottom: 50px;
    left: 0;
    width: 100%;
	height: 50px;
	line-height: 50px;
	text-align: center;
	font-size: 28rpx;
	overflow: hidden;
	background: #fff;
	display: flex;
	display: -webkit-flex;
	align-items: center;
	justify-content: space-between;
}

.bottom-btns {
	bottom: 0;
    z-index: 10;
	color: #fff;
	background-color: #5cb5ff;
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #74c0fb), color-stop(100%, #48a1f9));
    background: -webkit-linear-gradient(top, #74c0fb 0%, #48a1f9 100%);
    background: linear-gradient(to bottom, #74c0fb 0%, #48a1f9 100%);
}

.td {
	width: 33%;
    font-size: 28rpx;
    color: #2e2e2e;
    text-align: left;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
.td:nth-child(2){
	text-align: center;
}
.td:last-child{
	text-align: right;
}

/* student or agent table and select end */
